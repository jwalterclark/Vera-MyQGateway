<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>1</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable>
			<name>Username</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<stateVariable>
			<name>Password</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<stateVariable>
			<name>OpenerIDs</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
		</stateVariable>
		<stateVariable>
			<name>RefreshInterval</name>
			<dataType>i4</dataType>
			<defaultValue>120</defaultValue>
		</stateVariable>
		<stateVariable>
			<name>RefreshAfterChangeInterval</name>
			<dataType>i4</dataType>
			<defaultValue>10</defaultValue>
		</stateVariable>
		<stateVariable>
			<name>DebugMode</name>
			<dataType>boolean</dataType>
			<defaultValue>false</defaultValue>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>ToggleDebugMode</name>
			<argumentList></argumentList>
		</action>
		<action>
			<name>SetTarget</name>
		</action>
		<action>
			<name>SetCredentials</name>
			<argument>
				<name>newUsername</name>
				<direction>in</direction>
				<relatedStateVariable>Username</relatedStateVariable>
			</argument>
			<argument>
				<name>newPassword</name>
				<direction>in</direction>
				<relatedStateVariable>Password</relatedStateVariable>
			</argument>
		</action>
	</actionList>
</scpd>
